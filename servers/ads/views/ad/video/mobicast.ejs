<html>
  <head>
    <style>
      html, body {
        background: transparent;
        padding: 0;
        margin: 0;
        overflow: hidden;
        font-family: "Arial";
        background: black;
      }
      
      .mb-widget .tag {
        display: none !important;
      }
      
      .close {
        position: fixed;
        top: 20px;
        right: 20px;
        background: #F6F6F6;
        color: black;
        padding-top: 7px;
        text-align: center;
        border-radius: 50%;
        width: 40px;
        height: 40px;
        box-sizing: border-box;
        font-size: 1.4em;
        font-weight: bold;
      }
      
    </style>
  </head>
  <body>
    <div data-mb-widget data-id="115" data-account="14" data-style="responsive" data-autoplay></div>
    <script type="text/javascript" src="//widget-demo.mobicast.io/main.js?eeb2ac7f636fb3e5d1d3"></script>
    <img style="display: none;" src="/<%- creative.attributed_link(publisher.id, creative.industry_id, is_protected) %>"/>
    <div class="close" onclick="hideFullscreen()">x</div>
  </body>
  <script>
    function hideFullscreen() {
      window.parent.window.postMessage({
        name: "fullscreen.remove",
        frame: "<%= frame %>"
      }, "*");
    }
  </script>
</html>