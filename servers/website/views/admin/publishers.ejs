<% include ../includes/start %>
<% include ../includes/header %>
<div class="container large-2 publishers">
  <table>
    <head>
      <tr>
        <th>Name</th>
        <th>Domain</th>
        <th>API Key</th>
        <th>Coverage</th>
        <th>Industry</th>
        <th></th>
      </tr>
    </head>
    <tbody>
      <% publishers.forEach(function(publisher) { %>
        <tr>
          <td><%= publisher.name %></td>
          <td><a target="_blank" href="http://<%= publisher.domain %>"><%= publisher.domain %></a></td>
          <td><%= publisher.key %></td>
          <td><%= publisher.coverage_ratio * 100 %>%</td>
          <td><%= publisher.industry.name %></td>
          <td><a target="_blank" class="fa fa-external-link" href="/dashboard/<%= publisher.key %>/analytics"></a></td>
        </tr>
      <% }) %>
    </tbody>
  </table>
</div>
<% include ../includes/end %>