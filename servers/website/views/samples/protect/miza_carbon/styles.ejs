<style type="text/css">
  <% network = LIBS.models.defaults.carbon_network.id %>
  
  #carbonads .carbon-wrap,
  #<%= publisher.key %>_<%= network %> .<%= publisher.key %>_<%= network %>-wrap {
    display: block;
    width: 150px;
  }
  
  #carbonads .carbon-img,
  #<%= publisher.key %>_<%= network %> .<%= publisher.key %>_<%= network %>-img {
    display: block;
  }
  
  #carbonads .carbon-img img,
  #<%= publisher.key %>_<%= network %> .<%= publisher.key %>_<%= network %>-img img {
    display: block;
  }
  
  #carbonads .carbon-text,
  #<%= publisher.key %>_<%= network %> .<%= publisher.key %>_<%= network %>-text {
    clear: both;
    display: block;
    padding: 10px 0 0;
    color: #424249;
    font-size: 12px;
    line-height: 1.5em
  }
  
  #carbonads .carbon-poweredby,
  #<%= publisher.key %>_<%= network %> .<%= publisher.key %>_<%= network %>-poweredby {
    display: block;
    color: #ADB0BA;
    font-size: .9em;
    padding: 10px 0 0
  }
</style>